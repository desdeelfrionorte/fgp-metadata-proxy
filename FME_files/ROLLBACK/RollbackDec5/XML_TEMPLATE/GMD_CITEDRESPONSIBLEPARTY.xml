           <gmd:citedResponsibleParty xmlns:gmd="http://www.isotc211.org/2005/gmd" xmlns:gco="http://www.isotc211.org/2005/gco">
            <gmd:CI_ResponsibleParty>
              <gmd:individualName xsi:type="gmd:PT_FreeText_PropertyType">
                <gco:CharacterString>{fme:get-attribute("individualName")}</gco:CharacterString>
              </gmd:individualName>
              <gmd:organisationName xsi:type="gmd:PT_FreeText_PropertyType">
                <gco:CharacterString>{fme:get-attribute("p-t_entity")}; {fme:get-attribute("p-t_entity")}; {fme:get-attribute("sector")}</gco:CharacterString>
                <gmd:PT_FreeText>
                  <gmd:textGroup>
                    <gmd:LocalisedCharacterString locale="{fme:get-attribute("p-t_entity_other_lang_locale")}">{fme:get-attribute("p-t_entity_other_lang")}; {fme:get-attribute("p-t_entity_other_lang")}; {fme:get-attribute("sector_fr")}</gmd:LocalisedCharacterString>
                  </gmd:textGroup>
                </gmd:PT_FreeText>
              </gmd:organisationName>
              <gmd:positionName xsi:type="gmd:PT_FreeText_PropertyType">
                <gco:CharacterString>{fme:get-attribute("contact_position")}</gco:CharacterString>
                <gmd:PT_FreeText>
                  <gmd:textGroup>
                    <gmd:LocalisedCharacterString locale="{fme:get-attribute("contact_position_other_lang_locale")}">{fme:get-attribute("contact_position_other_lang")}</gmd:LocalisedCharacterString>
                  </gmd:textGroup>
                </gmd:PT_FreeText>
              </gmd:positionName>
              <gmd:contactInfo>
                <gmd:CI_Contact>
                  <gmd:address>
                    <gmd:CI_Address>
                      <gmd:electronicMailAddress xsi:type="gmd:PT_FreeText_PropertyType">
                        <gco:CharacterString>{fme:get-attribute("contact_email")}</gco:CharacterString>
                        <gmd:PT_FreeText>
                          <gmd:textGroup>
                            <gmd:LocalisedCharacterString locale="{fme:get-attribute("contact_email_other_lang_locale")}">{fme:get-attribute("contact_email")}</gmd:LocalisedCharacterString>
                          </gmd:textGroup>
                        </gmd:PT_FreeText>
                      </gmd:electronicMailAddress>
                    </gmd:CI_Address>
                  </gmd:address>
                  {fme:process-features("GMD_ONLINE_RESOURCE")}
                </gmd:CI_Contact>
              </gmd:contactInfo>
              <gmd:role>
                <gmd:CI_RoleCode
                  codeList="http://nap.geogratis.gc.ca/metadata/register/napMetadataRegister.xml#IC_90"
                  codeListValue="{fme:get-attribute("contact_role")}">{fme:get-attribute("contact_role_en")}; {fme:get-attribute("contact_role_fr")}</gmd:CI_RoleCode>
              </gmd:role>
            </gmd:CI_ResponsibleParty>
           </gmd:citedResponsibleParty>